{
	"name": "calculate-js",
	"version": "1.0.0",
	"description": "A commonly used calculator written by JavaScript, including calculations related to finance, statistics, etc.",
	"main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "jsnext:main": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/imaluna/calculate-js"
	},
	"scripts": {
		"clean": "rimraf lib",
		"build": "npm run clean && rollup -c",
		"prettier": "prettier --write .",
		"lint": "eslint ./ --fix",
		"prepare": "husky install",
		"test": "jest",
		"coveralls": "jest --coverage"
	},
	"keywords": [
		"calculate",
		"calculator",
		"cal",
		"finance",
		"statistics",
		"unit conversion"
	],
	"author": "imaluna<https://github.com/imaluna/calculate-js.git",
	"homepage": "https://github.com/imaluna/calculate-js#readme",
	"private": false,
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"license": "ISC",
	"engines": {
		"node": ">=16.0.0"
	},
	"browserslist": [
		"last 2 versions",
		"> 5%",
		"not ie <= 9",
		"since 2013"
	],
	"files": [
		"lib"
	],
	"lint-staged": {
		"*.{js,ts,jsx}": [
			"npm run prettier",
			"npm run lint"
		],
		"*.{json,html,md}": [
			"prettier --write"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.24.7",
		"@babel/preset-env": "^7.24.7",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.4.1",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.2",
		"rimraf": "^5.0.7",
		"rollup": "^4.18.0",
		"tslib": "^2.6.2",
		"typescript": "5.3.2",
		"@types/jest": "^29.5.14",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5"
	}
}
